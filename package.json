  "scripts": {
    "dev": "concurrently -k -n \"Webpack:Client,Eslint,Webpack:Server,Node\" -p \"[{name}]\" -c \"blue,green,magenta,grey\" \"webpack --watch\" \"esw --watch --ext=.ts,.tsx src/js/*\" \"webpack --config webpack.server.js --mode development --watch\" \"nodemon ./server-build/index.js\"",
    "eslint": "eslint --ext=.ts,.tsx src/js/*",
    "build": "concurrently -n \"Webpack:Client,Webpack:Server\" -p \"[{name}]\" -c \"blue,magenta\" \"webpack\" \"webpack --config webpack.server.js --mode development\"",
    "release": "concurrently -n \"Webpack:Client,Webpack:Server\" -p \"[{name}]\" -c \"blue,magenta\" \"webpack --mode production\" \"webpack --config webpack.server.js --mode production\"",
    "start": "node ./server-build/index.js"
